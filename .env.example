# Runtime
ENVIRONMENT=dev
BASE_URL=http://localhost:8080
PORT=8080
SERVICE_NAME=jwebmp-agcharts
STARTUP_CPU_BOOST=false

# Auth / OIDC (optional if securing websocket channels)
OAUTH2_ISSUER_URL=https://auth.example.com
OAUTH2_REALM=ai-experiment
OAUTH2_CLIENT_ID=agcharts-client
OAUTH2_CLIENT_SECRET=replace-me
JWKS_URI=https://auth.example.com/realms/ai-experiment/protocol/openid-connect/certs
AUTH_ADMIN_USER=admin
AUTH_ADMIN_PASS=replace-me

# Observability / events
CLOUDEVENTS_FORMAT=application/cloudevents+json
TRACING_ENABLED=false
LOGGING_BACKEND=log4j2
METRICS_EXPORT=disabled
ENABLE_DEBUG_LOGS=false
